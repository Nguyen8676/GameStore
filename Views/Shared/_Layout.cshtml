<!DOCTYPE html>
<html>
<head style="background-color:gray">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
     <link href="~/Content/CSSuser/StyleSheet1.css" rel="stylesheet" />
</head>









<body>  
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("GameStore", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
              
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "GameStore")</li>
                    <li>@Html.ActionLink("About", "About", "GameStore")</li>
                    <li>@Html.ActionLink("new account", "Dangky", "Nguoidung")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <div class="row">
                    <!-- cot san pham theo the loai-->
                    @Html.Action("TheLoai", "GameStore")
                    <!-- cot san pham theo nha phat trien-->
                    @Html.Action("NhaPhatTrien", "GameStore")
                    <!-- cot san pham theo nha phat hanh-->
                    @Html.Action("NhaPhatHanh", "GameStore")
                </div>
            </div>
            <div class="col-md-9">
                <div class="container body-content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    

      
</body>
</html>
